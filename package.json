{
  "name": "@asod/field",
  "description": "",
  "version": "0.1.0-canary.4",
  "license": "MIT",
  "author": "Oleg Putseiko <oleg.putseiko@gmail.com> (https://github.com/oleg-putseiko)",
  "keywords": [],
  "homepage": "https://github.com/asodorg/field",
  "repository": {
    "type": "git",
    "url": "https://github.com/asodorg/field"
  },
  "bugs": {
    "url": "https://github.com/asodorg/field/issues"
  },
  "main": "dist/index.js",
  "files": [
    "dist/**/*"
  ],
  "exports": "./dist/index.js",
  "scripts": {
    "build": "yarn build:js && yarn build:ts",
    "build:js": "swc src -d dist --strip-leading-paths --ignore \"node_modules,**/*.test.ts,**/*.d.ts\"",
    "build:ts": "tsc && mkdir -p dist && find src -name \"*.d.ts\" -exec cp {} dist/ \\;",
    "lint": "eslint ./src/**/*.ts -c ./eslint.config.mjs",
    "lint:fix": "yarn lint --fix && yarn format:fix",
    "lint:strict": "yarn lint --max-warnings=0 && yarn format",
    "format": "prettier -c .",
    "format:fix": "prettier -w .",
    "typecheck": "tsc --noEmit --incremental false",
    "test": "jest",
    "prepare": "husky install | chmod +x ./.husky/*",
    "version:bump": "npm version -m \"build(package): bump the package version to %s\""
  },
  "dependencies": {
    "@asod/compare": "^0.1.1-canary.5",
    "@asod/core": "^0.1.0-canary.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@swc/cli": "^0.5.2",
    "@swc/core": "^1.10.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^10.1.1",
    "husky": "^8.0.3",
    "jest": "^30.2.0",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.3",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "volta": {
    "node": "20.19.5",
    "yarn": "4.10.3"
  }
}
